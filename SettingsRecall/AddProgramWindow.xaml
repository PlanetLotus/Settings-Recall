<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:SettingsRecall" x:Class="SettingsRecall.AddProgramWindow"
        Title="AddProgramWindow" Height="426" Width="427">
    <Grid>
        <Label Content="Program Name:" HorizontalAlignment="Left" Margin="10,15,0,0" VerticalAlignment="Top"/>
        <Button x:Name="AddFilesButton" Content="Add Files" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="307,47,0,0" Click="Button_Click_1"/>
        <ListBox x:Name="PathListBox" HorizontalAlignment="Left" Height="281" Margin="10,79,0,0" VerticalAlignment="Top" Width="399"/>
        <TextBox x:Name="ProgramNameText" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="176" Margin="102,19,0,0"/>
        <Label Content="Version Name:" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="VersionNameText" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="176" Margin="102,47,0,0"/>
        <Button x:Name="SaveProgramButton" Content="Save Program" HorizontalAlignment="Left" VerticalAlignment="Top" Width="83" Margin="326,365,0,0"/>
        <Button x:Name="DeleteButton" Content="Delete" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,365,0,0" Click="DeleteButton_Click"/>
    </Grid>
</Window>
