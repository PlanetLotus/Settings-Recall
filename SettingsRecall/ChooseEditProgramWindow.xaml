<Window x:Class="SettingsRecall.ChooseEditProgramWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Choose Program to Edit" Height="351.895" Width="295" WindowStartupLocation="CenterOwner">
    <Grid>
        <ListBox x:Name="programListBox" HorizontalAlignment="Left" Height="272" Margin="10,10,0,0" VerticalAlignment="Top" Width="267" SelectionChanged="programListBox_SelectionChanged"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="122,287,0,0" VerticalAlignment="Top" Width="75" Click="cancelButton_Click" IsCancel="True"/>
        <Button x:Name="OKButton" Content="OK" HorizontalAlignment="Left" Margin="202,287,0,0" VerticalAlignment="Top" Width="75" Click="OKButton_Click"/>

    </Grid>
</Window>
